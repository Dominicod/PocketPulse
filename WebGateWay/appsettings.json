{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "Identity" : {
        "ClusterId": "Identity",
        "Match": {
          "Path": "identity/{controller}/{action}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{controller}/{action}/{**catchall}"
          }
        ]
      }
    }
  }
}